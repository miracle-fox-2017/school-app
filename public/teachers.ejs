<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>Teachers</title>
</head>
<body>
    <div class="header">
        <h1>Teachers List</h1>
    </div>
    <div class="content">
        <%if(teachersRows.length === 0){%>
            <div class="empty">
                <span>Kosong</span>
            </div>
        <%}else{%>
            <table border="1">
                <tr>
                    <td>ID</td>
                    <td>Nama Depan</td>
                    <td>Nama Belakang</td>
                    <td>Alamat Email</td>
                </tr>
                <%teachersRows.map((value)=>{%>
                <tr>
                    <td><%=value.id%></td>
                    <td><%=value.first_name%></td>
                    <td><%=value.last_name%></td>
                    <td><%=value.email%></td>
                </tr>
                <%})%>
            </table>
        <%}%>
    </div>
</body>
</html>
