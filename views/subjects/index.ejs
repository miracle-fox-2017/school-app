<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Subjects Page</title>
  </head>
  <body>
    <a href="/">Home</a>
    <a href="/teachers">Teachers</a>
    <a href="/subjects">Subjects</a>
    <a href="/students">Students</a>

    <table>
      <thead>
        <tr>
          <th>Id</th>
          <th>Subject Name</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody>
        <% dataSubjects.forEach(data => { %>
          <tr>
            <td><%= data.id %></td>
            <td><%= data.subject_name %></td>
            <td>
              <a href="/subjects/edit/<%= data.id %>">Edit</a>
              <a href="/subjects/delete/<%= data.id %>">Delete</a>
            </td>
          </tr>
        <% }) %>
      </tbody>
    </table>
    <a href="/subjects/add">Add Subject</a>
  </body>
</html>
