<%-include("partials/header",{title:'SUBJECTS'})%>
  <body>
    <table border="1">
      <tr>
        <th>ID</th>
        <th>Subject Name</th>
        <th>Teachers List</th>
        <th>Actions</th>

      </tr>

      <%let i = 1;
       results.forEach((result) => {%>
         <tr>
         <td><%=i%></td>
         <td><%=result.subject_name%></td>
         <td>
         <%if (result.Teacher !== null) {
           result.Teachers.forEach(teacher => { %>
             <ul>
               <li>
                 <%=teacher.first_name%>
                 <%=teacher.last_name%>
               </li>
             </ul>
           <%})%>
           <td><a href="/subjects/<%=result.id%>/enrolledstudents">Enrolled Students</a></td>
         <%}%>
         </td>
         </tr>
       <%i++; })%>

    </table>

    <a href="/subjects/add">Add Subjects</a>
  </body>
</html>
