<%- include partial/head %>
      <div class="">
        <div class="text-center">
          <h1>All Subjects Data</h1>
        </div>

        <table class="table table-bordered">
          <thead>
            <tr>
              <th>ID</th>
              <th>Subject Name</th>
              <th>Teacher</th>
              <th>Actions</th>
            </tr>
          </thead>
          <% rows.forEach(data=>{%>
          <tbody>
            <tr>
              <td><%= data.id %></td>
              <td><%= data.subject_name %></td>
              <td>
              <% data.Teachers.forEach(dataTeacher=>{ %>
              <%= dataTeacher.first_name %> <%= dataTeacher.last_name %>  <br />
              <% }) %>
              </td>
              <td><a href="/subjects/<%= data.id %>/enrolledstudents ">Enrolled Students</a></td>
            </tr>
          </tbody>
          <% })  %>
        </table>
      </div>
    </div>
  </body>
</html>
