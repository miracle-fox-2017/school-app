<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Subjects</title>
  </head>
    <style media="screen">
    table, th, td {
    border: 1px solid black;
    }
    </style>
  <body>
    <table>
      <tr>
        <th>ID</th>
        <th>Subject</th>
        <th>Teacher</th>
        <th>Students</th>
        <th>Option</th>
      </tr>
        <% rows.forEach(function(row){%>
      <tr>
        <td> <%= row.id %> </td>
        <td> <%= row.subject_name %> </td>
        <td> <%= row.teacher %> </td>
        <td><a href="/subjects/<%=row.id%>/enrolledstudents">enrolledstudents</a></td>
      </tr>
      <% }) %>
    </table>

  </body>
</html>
