<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>Students</title>
</head>
<body>
	<h1 style="text-align: center;">Student Data</h1>

	<table width="100%" border="1">
		<thead>
			<tr>
				<th>Id</th>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Email</th>
			</tr>
		</thead>

		<tbody>
			<% students.forEach( function(student, index) { %>
				<tr>
					<td><%= student.id %></td>
					<td><%= student.first_name %></td>
					<td><%= student.last_name %></td>
					<td><%= student.email %></td>
				</tr>
			<% }); %>
		</tbody>
	</table>
</body>
</html>