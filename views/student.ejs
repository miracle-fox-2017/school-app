<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <p><a href="/students/add">add</a></p>
    <table>
      <tr>
        <th>id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Full Name</th>
        <th>Email</th>
      </tr>
      <%for(var i = 0; i < student.length; i++){%>
        <tr>
          <td><%=student[i].id%></td>
          <td><%=student[i].first_name%></td>
          <td><%=student[i].last_name%></td>
          <td><%=student[i].getfullname()%></td>
          <td><%=student[i].email%></td>
          <td><a href="/students/edit/<%=student[i].id%>">edit</a></td>
          <td><a href="/students/delete/<%=student[i].id%>">delete</a></td>
        </tr>
      <%}%>
    </table>
  </body>
</html>
