<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>ADD SUBJECT STUDENTS</title>
  </head>
  <body>
    <h1>ADD SUBJECT STUDENT</h1>
    <form method="post">
    </br>first_name : <input type="text" name="first_name" value="<%= students.first_name %>">
    <br>
    </br>last_name : <input type="text" name="last_name" value="<%= students.last_name %>">
    <br>
  </br>Email : <input type="text" name="email" value="<%= students.email %>">
  <select class="" name="SubjectId">
    <% subjects.map(row => { %>
      <option value="<%= row.id %>"><%= row.subject_name %></option>
    <% }) %>
  </select>
  <input type="submit" name="" value="Add Student Subject">
    </form>

  </body>
</html>
