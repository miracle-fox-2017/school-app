<%-include partial/index%>
    <table>
      <form class="" action="/subject" method="post">
        <tr>
          <td><input name="subject_name" placeholder="SUBJECT NAME"></td>
          <td><button type="submit" >ADD</button></td>
        </tr>
      </form>
    </table>
    <table border>
      <tr>
        <th>id</th>
        <th>Subject_name</th>
        <th>Teacher</th>
      </tr>
      <%for(let i = 0;i < rows.length ;i++){%>
        <tr>
          <td><%=rows[i].id%></td>
          <td><%=rows[i].subject_name%></td>
          <td>
            <% for(let j = 0;j < rows[i].teachers.length ;j++){%>
              <ol>
                <li><%=rows[i].teachers[j].getFullName()%><br></li>
              </ol>
            <%}%>
          </td>
          <td><a href="/subjects/<%=rows[i].id%>/enrolledstudents%>">enrolled student</a></td>
          <td><a href="/subject/delete/<%=rows[i].id%>">delete</a></td>
          <td><a href="/subject/edit/<%=rows[i].id%>">edit</a></td>
        </tr>
      <%}%>
    </table>
  </body>
</html>
