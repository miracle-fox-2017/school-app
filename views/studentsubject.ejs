<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>HALAMAN STUDENT SUBJECT</title>

    <style type = "text/css">

      .tabel {
        background-color: brown;
        font-family: Calibri;
        width : 100%;
      }

      .warna {
        color : white;
      }

      .rapi {
        color: white;
        text-align:  center;
      }

      fieldset {
        width : 60%;
      }

      a {
        color: white;
        font-family : Arial;
        text-decoration: none;
      }

    </style>

  </head>
  <body style="background-color : aquamarine">
    <fieldset>
      <legend><b> DATA STUDENT SUBJECT </b></legend>
        <table border = "1" class = "tabel">
          <thead class ="warna">
            <th><b>Id</b></th>
            <th><b>Student Id</b></th>
            <th><b>Subject Id</b></th>
          </thead>

      <tbody class = "rapi">
        <% for(let i = 0 ; i < rowStudentSubjects.length ; i++) { %>
          <tr>
            <td><%= rowStudentSubjects[i].id %></td>
            <td><%= rowStudentSubjects[i].StudentId %></td>
            <td><%= rowStudentSubjects[i].SubjectId %></td>
          </tr>
        <%}%>
      </tbody>
    </table>
  </fieldset>
  </body>
</html>
