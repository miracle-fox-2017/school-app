<%- include ../partials/header %>
  <body>
    <%- include ../partials/navbar %>

    <br><br>
    <form class="" action="/students/<%= dataStudent.id %>/addsubject" method="post">
      <label for="">First Name</label><br>
      <input type="text" name="first_name" value="<%= dataStudent.first_name %>" disabled><br>
      <br>
      <label for="">Last Name</label><br>
      <input type="text" name="last_name" value="<%= dataStudent.last_name %>" disabled><br>
      <br>
      <label for="">Email</label><br>
      <input type="text" name="email" value="<%= dataStudent.email %>" disabled><br>
      <br>
      <label for="">Adding Subject</label><br>
      <select class="" name="SubjectId">
        <% dataSubjects.forEach(dataSubject => { %>
          <option value="<%= dataSubject.id %>"><%= dataSubject.subject_name %></option>
        <% }) %>
      </select><br>
      <br>
      <input type="submit" name="" value="Submit">
    </form>
  </body>
</html>
