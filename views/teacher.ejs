<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Teachers</title>
  </head>
  <body>
    <h1>Teachers Data</h1>
    <table border=1 >
      <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
      </tr>
      <% for(let i = 0; i < teachData.length; i++){ %>
        <tr>
          <td><%= teachData[i].id %></td>
          <td><%= teachData[i].first_name %></td>
          <td><%= teachData[i].last_name %></td>
          <td><%= teachData[i].email %></td>
        </tr>
        <% } %>
    </table>
  </body>
</html>