<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <table>
      <tr>
        <th>id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
      </tr>
      <%for(var i = 0; i < teacher.length; i++) {%>
        <tr>
          <td><%=teacher[i].id%></td>
          <td><%=teacher[i].first_name%></td>
          <td><%=teacher[i].last_name%></td>
          <td><%=teacher[i].email%></td>
        </tr>
      <%}%>
    </table>
  </body>
</html>
