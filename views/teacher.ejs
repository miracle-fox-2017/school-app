<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>Daftar Teacher</title>
</head>
<body>
	<h1 style="text-align: center;">Teacher Data</h1>

	<table width="100%" border="1">
		<thead>
			<tr>
				<th>Id</th>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Email</th>
			</tr>
		</thead>

		<tbody>
			<% teachers.forEach( function(teacher, index) { %>
				<tr>
					<td><%= teacher.id %></td>
					<td><%= teacher.first_name %></td>
					<td><%= teacher.last_name %></td>
					<td><%= teacher.email %></td>
				</tr>
			<% }); %>
		</tbody>
	</table>
</body>
</html>